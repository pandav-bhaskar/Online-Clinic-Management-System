
@{
    ViewData["Title"] = "Index";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="initial-scale=1, width=device-width" />

    <link rel="stylesheet" href="~/css/Labtest.css" />
    <link rel="stylesheet" href="~/css/Login.css" />

    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css2?family=Inter:wght@100;200;300;400;500;600;700;800;900&display=swap" />
</head>
<body>
    <div class="lab-test2">
        <img class="labtest-1-icon" alt="" src="~/images/img_labtest1.png" />

        <button class="lab-test-child"></button>
        <button class="submit4">Submit</button>
        <input class="lab-test-item" type="text" />

        <input class="lab-test-inner" type="text" />

        <div class="lab-test3">Lab Test</div>
        <div class="lab-test4">Lab Test</div>
        <div class="test-cost">Test Cost</div>
        <img class="tooth-icon15" alt="" src="~/images/img_tooth_51x45.png" />

        <div class="clinic5">Clinic</div>
        <a asp-controller="Receptionist" asp-action="Index" class="receptionist7" id="receptionist">Receptionist</a>
        <a asp-controller="About" asp-action="Index" class="about5" id="about">About</a>
        <a asp-controller="Contact" asp-action="Index" class="contact5" id="contact">Contact</a>
        <a asp-controller="Home" asp-action="Index" class="register7" id="register">Logout</a>
        <a asp-controller="Laboratorian" asp-action="Index" class="laboratorian7" id="laboratorian">Laboratorian</a>
        <a asp-controller="Doctor" asp-action="Index" class="doctor7" id="doctor">Doctor</a>


    </div>
    <script>
        var receptionistText = document.getElementById("receptionistText");
        if (receptionistText) {
          receptionistText.addEventListener("click", function (e) {
            window.location.href = "./receptionist.html";
          });
        }

        var aboutText = document.getElementById("aboutText");
        if (aboutText) {
          aboutText.addEventListener("click", function (e) {
            window.location.href = "./about.html";
          });
        }

        var registerText = document.getElementById("registerText");
        if (registerText) {
          registerText.addEventListener("click", function (e) {
            window.location.href = "./register.html";
          });
        }

        var loginText = document.getElementById("loginText");
        if (loginText) {
          loginText.addEventListener("click", function (e) {
            window.location.href = "./login.html";
          });
        }

        var logoutText = document.getElementById("logoutText");
        if (logoutText) {
          logoutText.addEventListener("click", function (e) {
            window.location.href = "./login.html";
          });
        }

        var laboratorianText = document.getElementById("laboratorianText");
        if (laboratorianText) {
          laboratorianText.addEventListener("click", function (e) {
            window.location.href = "./laboratorian.html";
          });
        }

        var doctorText = document.getElementById("doctorText");
        if (doctorText) {
          doctorText.addEventListener("click", function (e) {
            window.location.href = "./doc-detail.html";
          });
        }
    </script>
</body>
</html>


